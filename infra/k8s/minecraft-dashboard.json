{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Tick Rate (TPS)",
      "description": "Ticks per second â€” healthy is 20. Derived from tick histogram.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(minecraft_tick_duration_seconds_count{job=\"minecraft-unified-metrics\"}[1m])",
          "legendFormat": "TPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 10 },
          "min": 0,
          "max": 22,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 15, "color": "yellow" },
              { "value": 19, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "title": "Players Online",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "minecraft_players_count{job=\"minecraft-unified-metrics\"}",
          "legendFormat": "Players",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "value": null, "color": "blue" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "title": "Server Uptime",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "(time() - process_start_time_seconds{job=\"minecraft-unified-metrics\"}) / 3600",
          "legendFormat": "Hours",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1
        }
      }
    },
    {
      "title": "MSPT (ms per tick)",
      "description": "Average milliseconds per tick. Over 50ms means server can't keep up with 20 TPS.",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "rate(minecraft_tick_duration_seconds_sum{job=\"minecraft-unified-metrics\"}[1m]) / rate(minecraft_tick_duration_seconds_count{job=\"minecraft-unified-metrics\"}[1m]) * 1000",
          "legendFormat": "MSPT",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 40, "color": "yellow" },
              { "value": 50, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Loaded Chunks",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 },
      "targets": [
        {
          "expr": "sum(minecraft_world_loaded_chunks{job=\"minecraft-unified-metrics\"})",
          "legendFormat": "Chunks",
          "refId": "A"
        }
      ]
    },
    {
      "title": "JVM Heap Memory",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "jvm_memory_heapmemoryusage_used{job=\"minecraft-jmx\"}",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "jvm_memory_heapmemoryusage_committed{job=\"minecraft-jmx\"}",
          "legendFormat": "Committed",
          "refId": "B"
        },
        {
          "expr": "jvm_memory_heapmemoryusage_max{job=\"minecraft-jmx\"}",
          "legendFormat": "Max",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
