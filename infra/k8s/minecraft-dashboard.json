{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Server TPS",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "minecraft_tps",
          "legendFormat": "TPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 10 },
          "min": 0,
          "max": 22,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 15, "color": "yellow" },
              { "value": 19, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "title": "Players Online",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "minecraft_players_count",
          "legendFormat": "Players",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "value": null, "color": "blue" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "title": "Server Uptime",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "jvm_runtime_uptime_ms / 1000 / 3600",
          "legendFormat": "Hours",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "h"
        }
      }
    },
    {
      "title": "MSPT (ms per tick)",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "minecraft_mspt",
          "legendFormat": "MSPT",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 40, "color": "yellow" },
              { "value": 50, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Loaded Chunks",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 },
      "targets": [
        {
          "expr": "sum(minecraft_loaded_chunks)",
          "legendFormat": "Chunks",
          "refId": "A"
        }
      ]
    },
    {
      "title": "JVM Heap Memory",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "jvm_memory_heapmemoryusage_used",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "jvm_memory_heapmemoryusage_committed",
          "legendFormat": "Committed",
          "refId": "B"
        },
        {
          "expr": "jvm_memory_heapmemoryusage_max",
          "legendFormat": "Max",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "GC Collections / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rate(jvm_gc_collectioncount[1m])",
          "legendFormat": "{{ collector }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "GC Pause Time / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "rate(jvm_gc_collectiontime[1m])",
          "legendFormat": "{{ collector }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "JVM Threads",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "jvm_threads_threadcount",
          "legendFormat": "Live",
          "refId": "A"
        },
        {
          "expr": "jvm_threads_daemonthreadcount",
          "legendFormat": "Daemon",
          "refId": "B"
        },
        {
          "expr": "jvm_threads_peakthreadcount",
          "legendFormat": "Peak",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Entities per World",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "minecraft_entities_count",
          "legendFormat": "{{ world }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "JVM Memory Pools",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "jvm_memory_pool_used",
          "legendFormat": "{{ pool }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Process CPU Load",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 32 },
      "targets": [
        {
          "expr": "jvm_os_processcpuload * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "value": null, "color": "green" },
              { "value": 60, "color": "yellow" },
              { "value": 85, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Open File Descriptors",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 32 },
      "targets": [
        {
          "expr": "jvm_os_openfiledescriptorcount",
