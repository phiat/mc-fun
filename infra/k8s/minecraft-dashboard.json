        {
          "expr": "jvm_threads_daemon{job=\"minecraft-jmx\"}",
          "legendFormat": "Daemon",
          "refId": "B"
        },
        {
          "expr": "jvm_threads_peak{job=\"minecraft-jmx\"}",
